/* components/news/news.wxss */
@import '../../style/font/iconfont';
.sylunbo, .sylunbo image {
  height: 300rpx;
  width: 750rpx;
}

.new-welfare>image {
  width: 100%;
}

.new-welfare>image:nth-of-type(1) {
  height: 152rpx;
}

.new-welfare>image:nth-of-type(2) {
  height: 206rpx;
  margin-top: -8rpx;
}

.categary-navs image {
  width: 375rpx;
  height: 200rpx;
}
.topic-case image {
  width: 750rpx;
  height: 400rpx;
}
.trans-layout{
  width:100%;
  height:70rpx;
}
.new-nav{
  display:flex;
  flex-flow:row nowrap;
  margin-top:-8rpx;
}
.new-nav image{
  flex:1;
  height:180rpx;
}
.national-day image{
  width:100%;
  margin-bottom:-8rpx;
}
.national-day image:nth-of-type(2){
  height:200rpx;
}
.national-day image:nth-of-type(4){
  height:80rpx;
}
.nice-act image{
  width:100%;
}
.nice-act image:nth-of-type(3){
  height:80rpx;
}
.goods-to{
  width:100%;
  height:160rpx;
}
.mid-lunbo{
  height:270rpx;
  background:#5f0daf;
  padding:10rpx 20rpx;
}
.mid-lunbo image{
  box-sizing:border-box;
  width:710rpx;
  border-bottom:2rpx solid #eaeaea;
  height:250rpx;
}
/* 小屈精选 */
.pop-item>image:nth-of-type(1){
  width:100%;
  height:85rpx;
}
.pop-item>image:nth-of-type(2){
  width:100%;
  height:256rpx;
}
/* 品牌好礼 */
.top-brands{
  overflow:auto;
  padding:10rpx 12rpx 30rpx 24rpx;
  background:#5f0daf;
  border-bottom:2rpx solid#fafafa;
  border-top:2rpx solid #fafafa;
}
.brands{
  width:228rpx;
  height:270rpx;
  margin-right:10rpx;
  margin-bottom:10rpx;
  border-radius:10rpx;
  overflow:hidden;
  box-sizing:border-box;
  background:#fafafa;
  float:left;
  position:relative;
}
.brands>image{
  width:228rpx;
  height:184rpx;
}
.brands>text{
  position:absolute;
  left:0;
  right:0;
  height:80rpx;
  bottom:0;
  text-align:center;
  line-height:80rpx;
  color:#ff4292;
  font-size:26rpx;
  z-index:200;
}
.brands>view{
  position:absolute;
  z-index:100;
  top:128rpx;
  left:25rpx;
  text-align:center;
  border-radius:50%;
  width:170rpx;
  height:170rpx;
  background:#fafafa;
  padding-top:30rpx;
}
.brands>view>image{
  width:114rpx;
  height:36rpx;
}
.double-ele image{
  width:100%;
  margin-bottom:-8rpx;
}
.double-ele>image:nth-of-type(1){
  height:650rpx;
}
.double-ele>image:nth-of-type(2){
  height:60rpx;
}
.double-ele>image:nth-of-type(3){
  height:180rpx;
}
/* 双十一爆款 */
.pre-ele>image:nth-child(1){
  width:100%;
  height:240rpx;
  margin-bottom:-8rpx;
}
.pre-ele>image:nth-child(2){
  width:100%;
  height:150rpx;
  margin-bottom:-8rpx;
}
.pop-hot>image:nth-child(1){
  height:110rpx;
  width:100%;
  margin-bottom:-8rpx;
}
.nav{
  height:80rpx;
  box-sizing:border-box;
  display:flex;
  flex-flow:nowrap row;
  align-items:center;
  justify-content:space-between;
  font-family: "Helvetica Neue",Helvetica,Arial,Sans-serif;
  background:#5f0daf;
  border-bottom:2rpx solid #eaeaea;
  border-top:2rpx solid #eaeaea;
  /* position:fixed;
  left:0;
  right:0;
  bottom:0; */
}
.nav>text{
  /* flex:1; */
  color:#fff;
  opacity:0.8;
  font-size:32rpx;
  font-weight:bold;
  /* line-height:80rpx; */
  margin:0 30rpx;
  padding:10rpx 0;
  text-align:center;
  font-family: "Helvetica Neue",Helvetica,Arial,Sans-serif;
}
.nav>.active{
  opacity:1;
  border-bottom:6rpx solid #fff;
}
/* 分会场 */
.parallel-session>image:nth-child(1){
  height:640rpx;
  width:100%;
  margin-bottom:-8rpx;
}
.parallel-session>image:nth-child(2){
  height:110rpx;
  width:100%;
  margin-bottom:-8rpx;
}
/* 畅销品类 */
.populars>image:nth-of-type(1){
  width:100%;
  height:110rpx;
  margin-bottom:-8rpx;
}
.populars>image:nth-of-type(2),.populars>image:nth-of-type(3),.populars>image:nth-of-type(4),.populars>image:nth-of-type(5),.populars>image:nth-of-type(6){
  width:100%;
  height:500rpx;
  margin-bottom:-8rpx;
}